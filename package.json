    "date-fns": "^2.8.0",
    "date-fns-timezone": "^0.1.4",
    "element-react": "^1.4.34",
    "element-theme-default": "^1.4.13",
    "formik": "^1.5.4",
    "graphql": "^14.3.0",
    "history": "^4.9.0",
    "lodash": "^4.17.14",
    "node-sass": "^4.13.1",
    "normalize.css": "^8.0.1",
    "path-to-regexp": "^3.0.0",
    "react": "^16.12.0",
    "react-apollo": "^2.5.5",
    "react-dom": "^16.8.6",
    "react-formik-ui": "^3.0.8",
    "react-helmet": "^5.2.1",
    "react-hot-loader": "^4.12.13",
